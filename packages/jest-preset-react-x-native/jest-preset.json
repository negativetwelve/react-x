{
  "moduleFileExtensions": ["js", "native.js"],
  "setupTestFrameworkScriptFile": "jest-preset-react-x-native/setup.js",
  "testEnvironment": "node",
  "testMatch": ["**/?(*.)spec.js", "**/?(*.)spec.native.js"],
  "transformIgnorePatterns": [
    "node_modules/(?!react-native|react-native-keychain|react-native-web|react-primitives|@react-native-community/async-storage/lib)"
  ]
}
