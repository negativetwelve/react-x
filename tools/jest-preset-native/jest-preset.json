{
  "moduleFileExtensions": ["js", "native.js"],
  "setupTestFrameworkScriptFile": "@react-x/jest-preset-native/setup.js",
  "testEnvironment": "node",
  "testMatch": ["**/?(*.)spec.js", "**/?(*.)spec.native.js"],
  "transformIgnorePatterns": [
    "node_modules/(?!react-native|react-native-keychain|react-native-web|react-primitives|@react-native-community/async-storage)"
  ]
}
